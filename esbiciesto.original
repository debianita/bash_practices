#!/bin/bash

echo "Por favor ingrese un año vamos a calcular si es biciesto"
read year
if [ $year % 4 == 0 ]; then
	elif [ $year % 100 !=  0 ]; then
		echo "El año $year es bisiesto"
	elif [ $year % 400 == 0 ]; then
		echo "El año $year es bisiesto"
	elif [ $year % 4 != 0 ]; then
		echo "El año $year es bisiesto"
	elif [ $year % 400 != 0 ]; then
		echo "El año $year NO es bisiesto"
	elif [ $year % 4 != 0 ]; then
		echo "El año $year NO es bisiesto"
fi	
